{*--------------------------------------------------------------------------------------------------------------SublimeRevenue HTML Front-End Template--------------------------------------------------------------------------------------------------------------Theme Name: Sublime Revenue--------------------------------------------------------------------------------------------------------------*}<!DOCTYPE html><html{if $language_direction == '1' } dir="rtl"{/if} class="no-js" lang="en"><head>    <meta charset="utf-8">    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">    <title>{$sitename} - {$header_title}</title>{*     <!-- mobile specific metas    ================================================== -->    <meta name="viewport" content="width=device-width, initial-scale=1"> *}    <!-- CSS    ================================================== -->    <link rel="stylesheet" href="/css/base.css">    <link rel="stylesheet" href="/css/vendor.css">    <link rel="stylesheet" href="/css/main.css">    <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">    <link rel="icon" href="/favicon.ico" type="image/x-icon">    <!-- JS    ================================================== -->    <script src="/js/modernizr.js"></script>    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>    <!-- Charts and graphs JS    ================================================== -->    <script src="/templates/source/common/morris_charts/js/raphael-min.js"></script>    <script src="/templates/source/common/morris_charts/js/morris.js"></script>    <!-- Chrome, Firefox OS and Opera --><meta name="theme-color" content="#532B72"><!-- Windows Phone --><meta name="msapplication-navbutton-color" content="#532B72"><!-- iOS Safari --><meta name="apple-mobile-web-app-status-bar-style" content="#532B72"></head><body style="background:{$background_color};" id="top"><!-- preloader================================================== --><div id="preloader">    <div id="loader" class="dots-jump">        <div></div>        <div></div>        <div></div>    </div></div><!-- header================================================== --><header class="s-header">    <div class="clearfix"></div>    <div class="content-area col-lg-12 col-md-12 nopad">        <div class="cnt">            {if isset($logout_msg)}                <div class="row">                    <div class="col-md-12" style="margin-top:30px;margin-left:30px;">                        <div class="alert-box alert-box--info hideit">                            {$logout_msg}                    <i class="fa fa-times alert-box__close" aria-hidden="true"></i>                        </div>                    </div>                </div>            {/if}            <div class="highlight clearfix">            </div>        </div>    </div>    <nav class="header-nav">        <a href="#0" class="header-nav__close" title="{$custom_close}"><span>{$custom_close}</span></a>        <h3>{$custom_navigate_to}</h3>        <div class="header-nav__content">            <ul class="header-nav__social">                {section name=nr loop=$language_pack}                    <li>                        <a href="#"                           onclick="document.getElementById('idev_language').value = '{$language_pack[nr].value}'; document.getElementById('language_form').submit(); return false;"><img                                    src="/images/lang_flags/{$language_pack[nr].name}.png" width="22" height="16"                                    alt="{$language_pack[nr].name}" class="lang_flag"/>                            <span>{$language_pack[nr].name|replace:'Russian':'Русский'}</span></a>                    </li>                {/section}            </ul>            <form id="language_form" method="POST" action="">                <input name="csrf_token" value="{$csrf_token}" type="hidden"/>                <input type="hidden" id="idev_language" name="idev_language" value=""/>                <input name="lang_token" value="{$language_token}" type="hidden"/>            </form>            <ul class="header-nav__list">                <li><a class="smoothscroll" href="#home" title="{$header_indexLink}"><i                                class="fa fa-home fa-fw"></i> {$header_indexLink}</a></li>                <li><a class="smoothscroll" href="#publishers" title="{$custom_publishers}"><i                                class="fa fa-user-circle fa-fw"></i> {$custom_publishers}</a></li>                <li><a class="smoothscroll" href="#advertisers" title="{$custom_advertisers}"><i                                class="far fa-user-circle fa-fw"></i> {$custom_advertisers}</a></li>                <li><a class="smoothscroll" href="#statistics" title="{$custom_statistics}"><i                                class="fas fa-chart-bar fa-fw"></i> {$custom_statistics}</a></li>                <li><a href="/blog" title="{$custom_blog}"><i                                class="fa fa-newspaper fa-fw"></i> {$custom_blog}</a></li>                {if !isset($affiliateUsername)}                    <li><a href="/login" title="{$custom_login}"><i class="fa fa-sign-in-alt fa-fw"></i> {$custom_login}                        </a></li>                {/if}                {if isset($affiliateUsername)}                    <li><a href="/dashboard" title="{$custom_dashboard}"><i                                    class="fas fa-tachometer-alt fa-fw"></i> {$custom_dashboard}</a></li>                {/if}                {if !isset($affiliateUsername)}                    <li><a href="/signup" title="{$header_signupLink}" style="color:#532B72;"><i                                    class="fa fa-user-plus fa-fw" style="text-shadow: none !important;"></i> {$header_signupLink}</a></li>                {/if}                {if isset($use_faq) && ($faq_location == 1)}                    <li><a href="/faq" title="{$menu_faq}"><i class="fa fa-question-circle fa-fw"></i> {$menu_faq}                        </a></li>                {/if}                {if isset($contact_form)}                    <li><a href="javascript:void(Tawk_API.toggle())" title="{$header_emailLink}"><i                                    class="fa fa-envelope fa-fw"></i> {$header_emailLink}</a></li>                {/if}                {if isset($affiliateUsername)}                    <li>                        <a href="/logout" title="{$menu_logout}"><i                                    class="fas fa-sign-out-alt fa-fw"></i> {$menu_logout}</a>                    </li>                {/if}            </ul><div class="addthis_inline_follow_toolbox addthisfloats"></div>        </div> <!-- end header-nav__content -->    </nav> <!-- end header-nav -->    <a class="header-menu-toggle" href="#0">        <span class="header-menu-icon"></span>    </a></header> <!-- end s-header --><!-- home================================================== --><div class="wrapper"><section id="home" class="s-home page-hero target-section" data-parallax="scroll">    <div class="corner-ribbon top-left sticky purple shadow">{$custom_closed_beta}</div>    <div class="grid-overlay">        <div>        </div>    </div>    <div class="sliding-background">    </div>    <div class="home-content">        <div class="row home-content__main">            <a href="/"><img src="/images/logo_big.svg" width="735" height="86" alt="{$sitename} - {$header_title}"/></a>            <h1>                {$header_title}            </h1>            <h3>                {$custom_we_build_brands} <span class="flicker-1">{$custom_globally}</span>            </h3>                        <div class="home-content__button">                <a href="/signup" class="btn btn--primary btn--large">                    {$header_signupLink}                </a>                <a href="#publishers" class="smoothscroll btn btn--large">                    {$custom_more_about_us}                </a>            </div>        </div> <!-- end home-content__main --><div class="addthis_inline_follow_toolbox" style="margin:3.6rem;width:0%;bottom:0.6rem;position:absolute;"></div>        <div class="home-content__scroll">            <a href="#publishers" class="scroll-link smoothscroll">                {$custom_scroll}            </a>        </div>    </div> <!-- end home-content --></section> <!-- end s-home --></div><!-- about================================================== --><section id="publishers" class="s-publishers target-section">    <div class="row section-header bit-narrow" data-aos="fade-up">        <div class="col-full">            <h3 class="subhead">{$custom_publishers}</h3>            <h1 class="display-1">                {$custom_sublime_revenue_is}            </h1>        </div>    </div> <!-- end section-header -->    <div class="row bit-narrow" data-aos="fade-up">        <div class="col-full">            <p class="lead">                {$custom_we_design}            </p>        </div>    </div> <!-- end about-desc -->    <div class="row bit-narrow">        <div class="about-process process block-1-2 block-tab-full">            <div class="col-block item-process" data-aos="fade-up">                <div class="item-process__text">                    <h4 class="item-title">{$index_heading_1}</h4>                    <p>                        {$index_paragraph_1}                    </p>                </div>            </div>            <div class="col-block item-process" data-aos="fade-up">                <div class="item-process__text">                    <h4 class="item-title">{$index_heading_2}</h4>                    <p>                        {$index_paragraph_2}                    </p>                </div>            </div>            <div class="col-block item-process" data-aos="fade-up">                <div class="item-process__text">                    <h4 class="item-title">{$index_table_title}</h4>                    <p>                    <table>                        <tbody>                        {if isset($details_show_type)}                            <tr>                                <td>{$index_table_commission_type}</td>                                <td>{$commission_type_info|replace:'65%':'16.25%'}, 1-click, 2-click, SMS/MO, Pin Submit, CC-submit, PPL (SOI, DOI), PPS, PPI, COD...</td>                            </tr>                            {* The following IF statements are only used if allowing affiliates to choose commission type. *}                            {if isset($choose_percentage_payout)}                                <tr>                                    <td>{$index_table_sale}:</td>                                    <td>                                        <div class="label label-danger">{$bot1}%</div>                                        {$index_table_sale_text}                                    </td>                                </tr>                            {/if}                            {if isset($choose_flatrate_payout)}                                <tr>                                    <td>{$index_table_sale}:</td>                                    <td>                                        <div class="label label-danger">{if $cur_sym_location == 1}{$cur_sym}{/if}{$bot2}{if $cur_sym_location == 2} {$cur_sym}{/if} {$currency}</div>                                        {$index_table_sale_text}                                    </td>                                </tr>                            {/if}                            {if isset($choose_perclick_payout)}                                <tr>                                    <td>{$index_table_click}:</td>                                    <td>                                        <div class="label label-danger">{if $cur_sym_location == 1}{$cur_sym}{/if}{$bot3}{if $cur_sym_location == 2} {$cur_sym}{/if} {$currency}</div>                                        {$index_table_click_text}                                    </td>                                </tr>                            {/if}                        {/if}                        {if isset($details_show_signup)}                            {if isset($add_balance_row)}                                <tr>                                    <td>{$index_table_initial_deposit}</td>                                    <td>{if $cur_sym_location == 1}{$cur_sym}{/if}{$init_deposit}{if $cur_sym_location == 2} {$cur_sym}{/if} {$currency}                                        - <font color="#CC0000"><b>{$index_table_deposit_tag}</b></font></td>                                </tr>                            {/if}                        {/if}                        {if isset($details_show_requirements)}                            {if isset($add_requirements_row)}                                <tr>                                    <td>{$index_table_requirements}</td>                                    <td>{if $cur_sym_location == 1}{$cur_sym}{/if}{$init_req}{if $cur_sym_location == 2} {$cur_sym}{/if} {$currency}                                        - {$index_table_requirements_tag}</td>                                </tr>                            {/if}                        {/if}                        {if isset($details_show_duration)}                            <tr>                                <td>{$index_table_duration}</td>                                <td>{$index_table_duration_tag}</td>                            </tr>                        {/if}                        </tbody>                    </table>                    </p>                </div>            </div>            <div class="col-block item-process" data-aos="fade-up">                <div class="item-process__text">                    <h4 class="item-title">{$index_heading_3}</h4>                    <p>                        {$index_paragraph_3}                    </p>                    <h4 class="item-title">{$custom_heading_four}!</h4>                    <p>                        {$custom_paragraph_four}                    </p>                    <div class="home-content__button">                        <a href="/signup" class="btn btn--primary btn--large heartbeat" style="width:25.5rem;">                            {$header_signupLink}                        </a>                        <a href="#statistics" class="smoothscroll btn btn btn--large" style="width:25.5rem;">                            {$custom_show_me_more}                        </a>                    </div>                </div>            </div>        </div> <!-- end process -->    </div> <!-- end row --></section> <!-- end s-publishers --><!-- services================================================== --><section id="advertisers" class="s-advertisers target-section darker">    <div class="row section-header bit-narrow" data-aos="fade-up">        <div class="col-full">            <h3 class="subhead">{$custom_advertisers}</h3>            <h1 class="display-1">                {$custom_our_services_are}            </h1>        </div>    </div> <!-- end section-header -->    <div class="row bit-narrow" data-aos="fade-up">        <div class="col-full">            <p class="lead">                {$custom_we_bring}            </p>        </div>    </div> <!-- end services-desc -->    <div class="row bit-narrow advertisers block-1-2 block-tab-full">        <div class="col-block item-service" data-aos="fade-up">            <div class="item-service__icon">                <i style="text-shadow: none !important;" class="fa fa-star fa-fw" style="color:#532B72;"></i>            </div>            <div class="item-service__text">                <h3 class="item-title">{$custom_brand_identity}</h3>                <p>{$custom_we_can}                </p>            </div>        </div>        <div class="col-block item-service" data-aos="fade-up">            <div class="item-service__icon">                <i style="text-shadow: none !important;" class="fa fa-code fa-fw" style="color:#532B72;"></i>            </div>            <div class="item-service__text">                <h3 class="item-title"> {$custom_integration}</h3>                <p>{$custom_do_you_own}                </p>            </div>        </div>        <div class="col-full" data-aos="fade-up">            <div class="home-content__button">                <a href="javascript:void(Tawk_API.toggle())" class="btn btn--primary btn--large heartbeat" style="width:25.5rem;">                    {$header_emailLink}                </a>                <a href="#statistics" class="smoothscroll btn btn btn--large" style="width:25.5rem;">                    {$custom_show_me_more}                </a>            </div>        </div>    </div> <!-- end services --></section> <!-- end s-advertisers --><!-- works================================================== --><section id="statistics" class="s-statistics target-section">    <div class="row section-header has-bottom-sep narrow target-section" data-aos="fade-up">        <div class="col-full">            <h3 class="subhead">{$custom_statistics}</h3>            <h1 class="display-1">                {$custom_we_create}            </h1>        </div>    </div> <!-- end section-header -->    {if isset($bar_comms_last_6) || isset($pie_top_5_month)}        <div class="row bit-narrow advertisers block-1-2 block-tab-full">            <div class="col-block item-service" data-aos="fade-up">                <div class="item-service__icon">                    <i style="text-shadow: none !important;" class="fa fa-calendar-alt fa-fw" style="color:#532B72;"></i>                </div>                <div class="item-service__text">                    <h3 class="item-title">{$custom_top_rpm_last_six_months}</h3>                    <p>                    <ul class="stats-tabs">                        {foreach $c_epc_l6m as $top}                            {counter name=l6mrpm assign='i'}                            <li><a href="#0">{$top["v"]|string_format:"%.4f"} €<em>#{$i}: <b>{$top["n"]}</b>                                        <img border="0" src="/images/geo_flags/{$top["n"]|lower}.png" alt="{$top["n"]}" class="geo_flag"/></em></a>                            </li>                            {if $i == 9}                                {break}                            {/if}                        {/foreach}                    </ul>                <ul class="skill-bars">                    <li>{assign "round1" "{$all_epc|string_format:"%.2f"|replace:'0.':''}"}                    <div class="progress percent{if {math equation="round( x / y ) * y" x={$round1} y=5} == 0}5{else}{math equation="round( x / y ) * y" x={$round1} y=5}{/if}"><span>{$all_epc|string_format:"%.4f"} €</span></div>                    <strong><i style="text-shadow: none !important;" class="fa fa-globe fa-fw"></i> {$custom_global}</strong>                     </li>                </ul>                <!--                <div class="last-updated">                 {$custom_last_updated}: {$c_epc_l6m_t}                </div>                -->                    </p>                </div>            </div>            <div class="col-block item-service" data-aos="fade-up">                <div class="item-service__icon">                        <i style="text-shadow: none !important;" class="fa fa-calendar-check fa-fw " style="color:#532B72;"></i>                </div>                <div class="item-service__text">                    <h3 class="item-title">{$custom_top_rpm_this_month}</h3>                    <p>                    <ul class="stats-tabs">                        {foreach $c_epc_tm as $top}                            {counter name=tmrpm assign='i'}                            <li><a href="#0">{$top["v"]|string_format:"%.4f"} €<em>#{$i}: <b>{$top["n"]}</b>                                        <img border="0" src="/images/geo_flags/{$top["n"]|lower}.png" alt="{$top["n"]}" class="geo_flag"/></em></a>                            </li>                            {if $i == 9}                                {break}                            {/if}                        {/foreach}                    </ul>                <ul class="skill-bars">                    <li>{assign "round2" "{$tm_all_epc|string_format:"%.2f"|replace:'0.':''}"}                    <div class="progress percent{if {math equation="round( x / y ) * y" x={$round2} y=5} == 0}5{else}{math equation="round( x / y ) * y" x={$round2} y=5}{/if}"><span>{$tm_all_epc|string_format:"%.4f"} €</span></div>                    <strong><i style="text-shadow: none !important;" class="fa fa-globe fa-fw"></i> {$custom_global}</strong>                    </li>                </ul>                <!--                <div class="last-updated">                 {$custom_last_updated}: {$c_epc_tm_t}                </div>                -->                    </p>                </div>            </div>            <div class="col-block item-service" data-aos="fade-up">                <div class="item-service__icon">                    <i style="text-shadow: none !important;" class="fa fa-calendar-alt fa-fw" style="color:#532B72;"></i>                </div>                <div class="item-service__text">                    <h3 class="item-title">{$custom_top_five_last_six_months}</h3>                    <p>                    <ul class="stats-tabs">                        {foreach $top_converting as $top}                            {counter name=l6msr assign='i'}                            <li><a href="#0">{$top["v"]|string_format:"%.3f"|replace:'.000':''}%<em>#{$i}: <b>{$top["n"]}</b>                                        <img border="0" src="/images/geo_flags/{$top["n"]|lower}.png" alt="{$top["n"]}" class="geo_flag"/></em></a>                            </li>                            {if $i == 9}                                {break}                            {/if}                        {/foreach}                    </ul>                <ul class="skill-bars">                    <li>{assign "round3" "{$total_sr|string_format:"%.2f"|replace:'0.':''}"}                    <div class="progress percent{math equation="round( x / y ) * y" x={$round3} y=5}"><span>{$total_sr|string_format:"%.3f"}%</span></div>                    <strong><i style="text-shadow: none !important;" class="fa fa-globe fa-fw"></i> {$custom_global}</strong>                    </li>                </ul>                <!--                <div class="last-updated">                 {$custom_last_updated}: {$c_sr_l6m_t}                </div>                -->                    </p>                </div>            </div>            <div class="col-block item-service" data-aos="fade-up">                <div class="item-service__icon">                        <i style="text-shadow: none !important;" class="fa fa-calendar-check fa-fw " style="color:#532B72;"></i>                </div>                <div class="item-service__text">                    <h3 class="item-title">{$custom_top_five_this_month}</h3>                    <p>                    <ul class="stats-tabs">                        {foreach $tm_top_converting as $top}                            {counter name=tmsr assign='i'}                            <li><a href="#0">{$top["v"]|string_format:"%.3f"|replace:'.000':''}%<em>#{$i}: <b>{$top["n"]}</b>                                        <img border="0" src="/images/geo_flags/{$top["n"]|lower}.png" alt="{$top["n"]}" class="geo_flag"/></em></a>                            </li>                            {if $i == 9}                                {break}                            {/if}                        {/foreach}                    </ul>                <ul class="skill-bars">                    <li>{assign "round4" "{$total_sr_tm|string_format:"%.2f"|replace:'0.':''}"}                    <div class="progress percent{math equation="round( x / y ) * y" x={$round4} y=5}"><span>{$total_sr_tm|string_format:"%.3f"}%</span></div>                    <strong><i style="text-shadow: none !important;" class="fa fa-globe fa-fw"></i> {$custom_global}</strong>                    </li>                </ul>                <!--                <div class="last-updated">                 {$custom_last_updated}: {$c_sr_tm_t}                </div>                -->                    </p>                </div>            </div>            <div class="col-block item-service" data-aos="fade-up">                <div class="item-service__icon">                    <i style="text-shadow: none !important;" class="fa fa-chart-pie fa-fw" style="color:#532B72;"></i>                </div>                <div class="item-service__text">                    <h3 class="item-title">{$custom_chart_today}</h3>                    <p style="color:#fff;">                        {if isset($pie_top_5_month)}                    <div class="row">                        <div class="portlet" style="border-color:{$portlet_3};">                            <div class="portlet-body">                                <div id="donut-daily-index"></div>                                {if !empty($d_top_affiliates)}                                {literal}                                    <script type="text/javascript">                                        Morris.Donut({                                            element: 'donut-daily-index',                                            resize: true,                                            data: [ {/literal}{$d_top_affiliates}{literal} ],                                            formatter: function (x, data) {                                                return data.formatted;                                            }                                        });                                    </script>                                {/literal}                                {else}                                    {$chart_this_month_none}                                {/if}                            </div>                        </div>                    </div>                    {/if}                    </p>                </div>            </div>            <div class="col-block item-service" data-aos="fade-up">                <div class="item-service__icon">                    <i style="text-shadow: none !important;" class="fa fa-chart-pie fa-fw" style="color:#532B72;"></i>                </div>                <div class="item-service__text">                    <h3 class="item-title">{$chart_this_month}</h3>                    <p style="color:#fff;">                        {if isset($pie_top_5_month)}                    <div class="row">                        <div class="portlet" style="border-color:{$portlet_3};">                            <div class="portlet-body">                                <div id="donut-monthly-index"></div>                                {if !empty($top_affiliates)}                                {literal}                                    <script type="text/javascript">                                        Morris.Donut({                                            element: 'donut-monthly-index',                                            resize: true,                                            data: [ {/literal}{$top_affiliates}{literal} ],                                            formatter: function (x, data) {                                                return data.formatted;                                            }                                        });                                    </script>                                {/literal}                                {else}                                    {$chart_this_month_none}                                {/if}                            </div>                        </div>                    </div>                    {/if}                    </p>                </div>            </div>            <div class="col-full item-service" data-aos="fade-up">                <div class="item-service__icon">                    <i style="text-shadow: none !important;" class="fa fa-chart-bar fa-fw" style="color:#532B72;"></i>                </div>                <div class="item-service__text">                    <h3 class="item-title">{$chart_last_6_months}</h3>                    <p>                        {if isset($bar_comms_last_6)}                    <div class="row">                        <div class="portlet" style="border-color:{$portlet_3};">                            <div class="portlet-body">                                <div id="bar-commissions-index"></div>                                {literal}                                <script type="text/javascript">                                    Morris.Bar({                                        element: 'bar-commissions-index',                                        data: [{                                            x: '{/literal}{$monthly_commissions[0].name}{literal}',                                            a: '{/literal}{$monthly_commissions[0].payouts}{literal}'                                        }, {                                            x: '{/literal}{$monthly_commissions[1].name}{literal}',                                         